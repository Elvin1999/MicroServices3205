<Window x:Class="UIApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UIApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <DataGrid SelectedItem="{Binding Contact}" ItemsSource="{Binding AllContacts}"></DataGrid>
            <Button Click="Button_Click">Get All Contacts</Button>
            <Button Click="Button_Click_1">Delete After Select Element</Button>
        </StackPanel>
    </Grid>
</Window>
